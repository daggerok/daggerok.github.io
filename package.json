    "angular2-highlight-js": "5.0.2",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "jquery": "2.2.4",
    "material-design-icons": "3.0.1",
    "materialize-css": "0.100.0",
    "roboto-fontface": "0.8.0",
    "rxjs": "5.1.0",
